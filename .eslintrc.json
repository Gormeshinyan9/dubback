{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/warnings",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-undef": "error",
    "no-unused-vars": "error",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "import/newline-after-import": "warn"
  }
}
